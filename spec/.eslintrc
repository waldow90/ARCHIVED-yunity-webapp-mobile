{
  "parser": "babel-eslint",
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "no-alert": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [],
  "globals": {
    "describe" : false,
    "it": false,
    "beforeEach": false,
    "afterEach": false,
    "expect": false
  }
}